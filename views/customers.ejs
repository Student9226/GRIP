<%- include('partials/header') %>
<h2>All Customers</h2>
<ul>
  <% customers.forEach(customer => { %>
    <li>
      <a href="/customer/<%= customer._id %>"><%= customer.name %></a>
      - Email: <%= customer.email %>
      - Current Balance: <%= customer.current_balance %>
    </li>
  <% }) %>
</ul>
<%- include('partials/footer') %>
